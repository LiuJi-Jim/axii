{
  "name": "axii",
  "version": "2.8.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "vite",
    "build": "vite build --config vite.prod.config.ts && node scripts/type.js",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "badge": "coverage-badges --style=flat",
    "release": "node scripts/release.js"
  },
  "author": "sskyy<skyking_H@hotmail.com>",
  "license": "MIT",
  "types": "./dist/axii.d.ts",
  "typings": "./dist/axii.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/axii.js",
      "require": "./dist/axii.umd.cjs",
      "types": "./dist/axii.d.ts"
    },
    "./jsx-runtime": {
      "import": "./dist/axii.js",
      "require": "./dist/axii.umd.cjs",
      "types": "./dist/axii.d.ts"
    },
    "./jsx-dev-runtime": {
      "import": "./dist/axii.js",
      "require": "./dist/axii.umd.cjs",
      "types": "./dist/axii.d.ts"
    }
  },
  "type": "module",
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.9.3",
    "@vitest/coverage-v8": "^3.0.5",
    "coverage-badges-cli": "^2.1.0",
    "typescript": "5.6",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.0.3",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^3.0.5",
    "@vitest/browser": "^3.0.5"
  },
  "peerDependencies": {
    "data0": "^1.8.0"
  }
}
